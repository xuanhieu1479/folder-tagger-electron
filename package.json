{
  "name": "folder-tagger-electron",
  "productName": "folder-tagger-electron",
  "version": "1.0.2",
  "description": "Manage folders with your own tags",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "env-cmd electron-forge publish"
  },
  "keywords": [],
  "author": {
    "name": "Pham Xuan Hieu",
    "email": "xuanhieu1479@gmail.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {
        "executableName": "Folder Tagger",
        "name": "Folder Tagger"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "win32"
          ]
        }
      ],
      "publishers": [
        {
          "name": "@electron-forge/publisher-github",
          "platforms": [
            "win32"
          ],
          "config": {
            "repository": {
              "owner": "xuanhieu1479",
              "name": "folder-tagger-electron"
            }
          }
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/renderer.ts",
                  "name": "main_window"
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "dependencies": {
    "@blueprintjs/core": "^3.36.0",
    "@blueprintjs/select": "^3.15.0",
    "@types/cors": "^2.8.9",
    "axios": "^0.21.1",
    "better-sqlite3": "^7.1.2",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "lodash": "^4.17.20",
    "moment": "^2.29.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-scripts": "4.0.1",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.19",
    "typeorm": "^0.2.29"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "@electron-forge/plugin-webpack": "6.0.0-beta.54",
    "@electron-forge/publisher-github": "^6.0.0-beta.54",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "@types/better-sqlite3": "^5.4.1",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/express": "^4.17.9",
    "@types/ini": "^1.3.30",
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.14.14",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.15",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "copy-webpack-plugin": "^6.4.1",
    "css-loader": "^4.2.1",
    "electron": "^11.1.1",
    "electron-devtools-installer": "^3.1.1",
    "electron-squirrel-startup": "^1.0.0",
    "env-cmd": "^10.1.0",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-prettier": "^3.3.0",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^5.0.14",
    "node-loader": "^1.0.1",
    "node-pre-gyp": "^0.17.0",
    "node-sass": "^5.0.0",
    "prettier": "^2.2.1",
    "redux-devtools-extension": "^2.13.8",
    "sass": "^1.30.0",
    "sass-loader": "^10.1.0",
    "style-loader": "^1.2.1",
    "ts-loader": "^8.0.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.0.2"
  }
}
